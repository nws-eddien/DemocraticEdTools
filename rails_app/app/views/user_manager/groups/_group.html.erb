<div id="<%= dom_id group %>">
  <%= turbo_frame_tag "group_#{group.id}" do%>
    <tr>
      <td><%= link_to group.name, user_manager_group_path(group) %>
      <td>
        <%= group.users_count %>
      </td>
      <td>
        <%= link_to :edit, edit_user_manager_group_path(group), class: "btn btn-yellow"%>
      </td>

    </tr>

  <% end %>

</div>
