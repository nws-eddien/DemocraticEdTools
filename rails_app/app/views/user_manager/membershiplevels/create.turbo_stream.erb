<%= turbo_stream.update Membershiplevel.new, ""%>

<% if @membershiplevel.previous_membershiplevel %>
  <%= turbo_stream.after @membershiplevel.previous_membershiplevel do  %>
    <%= render @membershiplevel, group: @group %>
  <% end %>
<% else %>
  <%= turbo_stream.prepend "membershiplevels" do %>
    <%= render @membershiplevel, group: @group %>
  <% end %>
<% end %>

<%= render_turbo_stream_flash_messages %>