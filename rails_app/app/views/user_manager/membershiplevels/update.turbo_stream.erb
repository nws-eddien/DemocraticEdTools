<%= turbo_stream.remove @membershiplevel%>

<% if previous_membershiplevel = @membershiplevel.previous_membershiplevel %>
  <%= turbo_stream.after previous_membershiplevel do  %>
    <%= render @membershiplevel, group: @group %>
  <% end %>
<% else %>
  <%= turbo_stream.prepend "membershiplevels" do %>
    <%= render @membershiplevel, group: @group %>
  <% end %>
<% end %>

<%= render_turbo_stream_flash_messages %>